{
  "name": "phone-order-for-woocommerce",
  "version": "2.0.0",
  "description": "Fast order creation with just a phone number for WooCommerce",
  "author": "OpenWPClub",
  "license": "GPL-2.0-or-later",
  "scripts": {
    "build": "wp-scripts build src-js/blocks/phone-order/index.js src-js/blocks/phone-order/view.js --output-path=build/phone-order-block",
